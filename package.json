{
  "name": "zef",
  "displayName": "Zef",
  "description": "Run Python code blocks from .zef.md files",
  "version": "0.0.2",
  "publisher": "zef",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zef.runBlock",
        "title": "Zef: Run Code Block"
      },
      {
        "command": "zef.runBlockAtCursor",
        "title": "Zef: Run Code Block at Cursor"
      },
      {
        "command": "zef.openPreview",
        "title": "Zef: Open Preview to Side"
      }
    ],
    "keybindings": [
      {
        "command": "zef.runBlockAtCursor",
        "key": "shift+enter",
        "when": "editorLangId == markdown && resourceFilename =~ /\\.zef\\.md$/"
      },
      {
        "command": "zef.openPreview",
        "key": "cmd+shift+v",
        "when": "editorLangId == markdown && resourceFilename =~ /\\.zef\\.md$/"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "marked": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0"
  }
}
